        <!--==================== MAIN ====================-->
        <main class="main">
            <!--==================== HOME ====================-->
            <section class="home section" id="home">
                <div class="home__container container grid">
                    <div class="home__data">
                        <h2 class="home__subtitle">HAZ QUE TU CUERPO</h2>
                        <h1 class="home__title">ESTÉ EN FORMA</h1>
                        <p class="home__description">Asociación de Fisicoculturismo, Fitness natural y Deportes afines.</p>
                        <a href="#" class="button button__flex">
                            Comenzar <i class="ri-arrow-right-line"></i>
                        </a>
                    </div>
                    <div class="home__images">
                        <img src="/img/personas-fisicoculturistas-sfondo.png" alt="foto fisicoculturistas" class="home__image">

                        <div class="home__triangle home__triangle-3"></div>
                        <div class="home__triangle home__triangle-2"></div>
                        <div class="home__triangle home__triangle-1"></div>
                    </div>
                </div>
            </section>

            <!--==================== LOGOS ====================-->
            <section class="logos section">
                <div class="logos__container container grid">
                    <img src="/img/fitdesa-logo-sfondo.png" alt="Logo Fitdesa" class="logos__img">
                    <img src="/img/adn-logo-sfondo.png" alt="Logo ADN Fitness" class="logos__img">
                    <img src="/img/dharma-logo-sfondo.png" alt="Logo Dharma" class="logos__img">
                    <img src="/img/finda-logo-sfondo.png" alt="Logo Finda" class="logos__img">
                </div>
                
            </section>

